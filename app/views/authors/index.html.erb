<p id="notice"><%= notice %></p>

<h1>Authors</h1>

<table class="table">
  <thead>
    <tr>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @authors.each do |author| %>
      <tr>
        <td><%= author.id %>
        <td><%= author.name %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br>
<%= paginate @authors %>
<br>
